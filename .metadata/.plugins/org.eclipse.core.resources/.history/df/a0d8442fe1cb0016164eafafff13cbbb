package aaa;

import java.util.Random;

public class aaa {

	public static void main(String[] args) {
		Random ran = new Random();
		int a[] = new int[10];
		int max = 0;
		int ave = 0;
		for (int i = 0; i < 10; i++) {
			a[i] = ran.nextInt(100);
		}
		for(int i=0;i<10;i++){
			if(a[i]>a[i+1]){
				max = a[i];
			}
		}
	}
}
